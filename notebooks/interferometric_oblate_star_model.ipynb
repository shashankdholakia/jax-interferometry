      "#ELR11 equations\n",
      "#gives the value of phi\n",
      "def eq24(phi,theta,omega,rtw):\n",
      "    tau = (jnp.power(omega,2) * jnp.power(rtw*jnp.cos(theta),3) )/3.0 + jnp.cos(theta) + jnp.log(jnp.tan(0.5*theta))\n",
